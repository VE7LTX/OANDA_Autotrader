# Copy to .env and fill in real values.

# Tokens (separate for demo/practice and live)
# DEMO_OANDA_API_KEY=
# LIVE_OANDA_API_KEY=

# Optional account ID overrides (if you want to pick from .env)
# DEMO_OANDA_ACCOUNT_ID=
# LIVE_OANDA_ACCOUNT_ID=

# Base URLs (optional overrides)
# OANDA_API_BASE_PRACTICE=https://api-fxpractice.oanda.com
# OANDA_API_BASE_LIVE=https://api-fxtrade.oanda.com

# Stream base URLs (optional overrides)
# OANDA_STREAM_BASE_PRACTICE=https://stream-fxpractice.oanda.com
# OANDA_STREAM_BASE_LIVE=https://stream-fxtrade.oanda.com

# Performance & reliability tuning (optional)
# OANDA_REQUEST_TIMEOUT_SECONDS=30
# OANDA_REQUESTS_PER_SECOND=100
# OANDA_DEBUG_LOGGING=false
# OANDA_STREAM_TIMEOUT_SECONDS=0
# OANDA_STREAM_RECONNECT=true
# OANDA_STREAM_MAX_RETRIES=
# OANDA_STREAM_BACKOFF_BASE_SECONDS=0.5
# OANDA_STREAM_BACKOFF_MAX_SECONDS=15.0

# Dashboard (optional)
# OANDA_DASHBOARD_GROUP=live
# OANDA_DASHBOARD_ACCOUNT=Primary
# OANDA_DASHBOARD_INSTRUMENT=USD_CAD
# OANDA_DASHBOARD_PRED_PATH=data/predictions_latest.jsonl
# OANDA_DASHBOARD_PRED_INTERVAL=5
# OANDA_DASHBOARD_AE_STATUS_PATH=data/ae_status.jsonl
# OANDA_DASHBOARD_AE_STATUS_INTERVAL=5
# OANDA_DASHBOARD_RECON_PATH=data/recon.jsonl
# OANDA_DASHBOARD_RECON_INTERVAL=5
# OANDA_DASHBOARD_LOG_PATH=data/dashboard.log
# OANDA_DASHBOARD_IGNORE_QUIT=false
# OANDA_DASHBOARD_AUTOSTART=true
# OANDA_DASHBOARD_PRED_FEATURES=data/usd_cad_features.jsonl
# OANDA_DASHBOARD_PRED_INTERVAL_SECS=5
# OANDA_DASHBOARD_PRED_HORIZON=12
# OANDA_DASHBOARD_PRED_EPOCHS=1
# OANDA_DASHBOARD_PRED_RETRAIN_INTERVAL=60
# OANDA_DASHBOARD_PRED_ARCHIVE=false
# OANDA_DASHBOARD_PRED_LOG=data/prediction_runner.log
# OANDA_DASHBOARD_SCORE_EVERY=10
# OANDA_DASHBOARD_SCORE_LOG=data/score_runner.log
