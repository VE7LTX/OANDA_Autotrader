# Copy to .env and fill in real values.

# Tokens (separate for demo/practice and live)
# DEMO_OANDA_API_KEY=
# LIVE_OANDA_API_KEY=

# Optional account ID overrides (if you want to pick from .env)
# DEMO_OANDA_ACCOUNT_ID=
# LIVE_OANDA_ACCOUNT_ID=

# Base URLs (optional overrides)
# OANDA_API_BASE_PRACTICE=https://api-fxpractice.oanda.com
# OANDA_API_BASE_LIVE=https://api-fxtrade.oanda.com

# Stream base URLs (optional overrides)
# OANDA_STREAM_BASE_PRACTICE=https://stream-fxpractice.oanda.com
# OANDA_STREAM_BASE_LIVE=https://stream-fxtrade.oanda.com

# Performance & reliability tuning (optional)
# OANDA_REQUEST_TIMEOUT_SECONDS=30
# OANDA_REQUESTS_PER_SECOND=100
# OANDA_DEBUG_LOGGING=false
# OANDA_STREAM_TIMEOUT_SECONDS=0
# OANDA_STREAM_RECONNECT=true
# OANDA_STREAM_MAX_RETRIES=
# OANDA_STREAM_BACKOFF_BASE_SECONDS=0.5
# OANDA_STREAM_BACKOFF_MAX_SECONDS=15.0
